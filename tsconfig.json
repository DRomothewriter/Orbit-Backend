{
    "compilerOptions": {
        "baseUrl": "src",
        "outDir": "dist",
        "target": "esnext",
        "module": "commonjs",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "allowSyntheticDefaultImports": true,
        "strict": false,
        "skipLibCheck": true
    }
}